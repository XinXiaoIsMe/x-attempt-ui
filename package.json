{
  "name": "x-attempt-ui",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@8.5.1",
  "description": "",
  "author": "XinXiaoIsMe https://github.com/XinXiaoIsMe",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "dev:docs": "pnpm --filter docs dev",
    "preview:docs": "pnpm --filter docs preview",
    "clear": "tsx build/src/clear",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "gen:icons": "pnpm --filter @x-attempt/icons gen:components",
    "build": "pnpm clear && concurrently \"pnpm build:comp\" \"pnpm build:hooks\" \"pnpm build:icons\"",
    "build:docs": "pnpm --filter docs build",
    "build:comp": "tsx build/src/build-component",
    "build:hooks": "tsx build/src/build-hooks",
    "build:icons": "concurrently \"pnpm gen:icons\" \"tsx build/src/build-icons\"",
    "new": "node scripts/new",
    "set:nojekyll": "pnpm --filter docs set:nojekyll",
    "release": "pnpm --dir build/src/build-component release",
    "release:hooks": "pnpm --dir build/src/build-hooks release",
    "release:icons": "pnpm --dir build/src/build-icons release"
  },
  "dependencies": {
    "@x-attempt/hooks": "workspace:*",
    "@x-attempt/icons": "workspace:*",
    "@x-attempt/ui": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.0",
    "@types/clean-css": "^4.2.11",
    "@types/fs-extra": "^11.0.4",
    "@types/minify": "^9.1.4",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "bumpp": "^9.4.0",
    "chalk": "^5.3.0",
    "clean-css": "^5.3.3",
    "concurrently": "^8.2.2",
    "consola": "^3.2.3",
    "eslint": "^8.57.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "tsx": "^4.7.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  }
}
